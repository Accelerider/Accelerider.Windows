<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    <SolidColorBrush x:Key="LightGreyBackgoundBrush" Color="#F1F2F4" />
    <SolidColorBrush x:Key="ListBoxItemMouseOverBackgroundBrush" Color="#7679BE"/>
    
    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#9EA3FE" po:Freeze="True" />
    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#DD000000" po:Freeze="True" />
    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#3F487F" po:Freeze="True" />
    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#DDFFFFFF" po:Freeze="True" />
    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#293851" po:Freeze="True" />
    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#DDFFFFFF" po:Freeze="True" />

</ResourceDictionary>